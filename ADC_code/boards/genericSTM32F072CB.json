        "extra_flags": "-DSTM32F0 -DSTM32F072xB -D PIO_FRAMEWORK_ARDUINO_ENABLE_CDC",
        "f_cpu": "48000000L",
        "mcu": "stm32f072cbt6",
        "product_line": "STM32F072xx",
        "variant": "STM32F0xx/F072C8(T-U)_F072CB(T-U-Y)"
    },
    "frameworks": [
        "arduino",
        "mbed",
        "stm32cube",
        "cmsis"
